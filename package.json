{
  "name": "vrls-tasks",
  "private": false,
  "version": "0.0.4",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "tasks",
    "queue",
    "async"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vitorrloureiro/vrls-tasks"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/vrls-tasks.umd.cjs",
  "module": "./dist/vrls-tasks.js",
