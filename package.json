    "bench": "vitest bench"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "vrls-simple-event-target": "^0.1.1"
